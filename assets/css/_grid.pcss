@layer base {
  .wrapper {
    min-height: 100vh;
    @apply my-0 mx-auto flex justify-center items-start flex-col;
  }

  main {
    @apply flex flex-col flex-auto w-full mx-auto pt-4 pb-16;
  }
  main.homepage {
    padding-bottom: 5rem;
    @apply flex flex-col flex-auto w-full mx-auto pt-0;
  }
}
